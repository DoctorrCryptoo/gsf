<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>GSF.Web.Security</id>
    <title>Grid Solutions Framework Web Application Security Template</title>
    <version>1.0.0.0</version>
    <authors>Grid Protection Alliance</authors>
    <description>Enables role-based security using Grid Solutions Framework security layer in an ASP.NET application.</description>
    <projectUrl>https://github.com/GridProtectionAlliance/gsf</projectUrl>
    <license type="expression">MIT</license>
    <tags>Web Application Security</tags>
    <icon>images\GSF64x64.png</icon>
    <dependencies>
      <dependency id="GSF.Web" />
    </dependencies>
  </metadata>
  <files>
    <file src="Content\**\*.*" target="Content" />
    <file src="..\..\..\Documentation\Images\GSF64x64.png" target="images\" />
  </files>
</package>